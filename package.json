{
  "name": "postman-env-templater",
  "description": "Creates postman environment files from env template files",
  "repository": "https://github.com/dkhunt27/postman-env-templater",
  "homepage": "",
  "author": "",
  "keywords": [],
  "private": false,
  "version": "0.0.6",
  "engines": {
    "node": ">=14"
  },
  "main": "./build/index.js",
  "files": [
    "src/bin/**/*",
    "build/lib/**/*",
    "build/index.*",
    "build/types/**/*"
  ],
  "bin": {
    "postman-env-templater": "src/bin/postman-env-templater"
  },
  "scripts": {
    "test": "yarn jest",
    "build": "rimraf build && tsc && tsc-alias",
    "preversion": "yarn lint && yarn build && yarn test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && npm run format && git add -A",
    "postversion": "git push --follow-tags && npm publish",
    "lint": "eslint --ext ts,js --fix src",
    "format": "prettier src/**/*.ts -w",
    "build-live": "tsc -w && tsc-alias -w"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.131.0",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.6",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "mock-fs": "^5.1.2",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.7",
    "tsc-alias": "^1.2.11",
    "typescript": "^4.7.4"
  }
}
